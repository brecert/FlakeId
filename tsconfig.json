{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "lib": [
      "esnext"
    ],
    "emitDeclarationOnly": true,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "outDir": "dist",
  }
}